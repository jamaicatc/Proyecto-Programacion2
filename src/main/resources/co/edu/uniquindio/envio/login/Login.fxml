<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="254.0" prefWidth="308.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.envio.viewcontroller.LoginViewController">
   <children>
      <VBox alignment="CENTER" prefHeight="342.0" prefWidth="406.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Iniciar Sesion" />
            <HBox alignment="CENTER" prefHeight="100.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER_RIGHT" prefWidth="80.0" text="Usuario:" HBox.hgrow="ALWAYS" />
                  <TextField fx:id="txtIngresarUsuario" maxWidth="-Infinity" promptText="Ingresar Usuario" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER_RIGHT" prefWidth="80.0" text="Contraseña:" />
                  <PasswordField fx:id="txtIngresarContrasena" maxWidth="-Infinity" promptText="Ingresar Contraseña" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER_RIGHT" prefWidth="80.0" text="Rol:" />
                  <ComboBox fx:id="cmbRol" prefWidth="150.0" promptText="Seleccionar">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></ComboBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button fx:id="btnIngresar" mnemonicParsing="false" onAction="#OnIngresar" text="Ingresar">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
                  <Button fx:id="btnCancelar" mnemonicParsing="false" onAction="#OnCancelar" text="Cancelar">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
