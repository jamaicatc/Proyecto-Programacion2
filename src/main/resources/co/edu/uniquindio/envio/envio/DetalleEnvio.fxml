                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="ID Envío:" />
                  <TextField fx:id="txtIdEnvio" editable="false" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" />
                  <Label text="Usuario:" GridPane.rowIndex="1" />
                  <TextField fx:id="txtUsuario" editable="false" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" />
                  <Label layoutX="10.0" layoutY="47.0" text="Repartidor:" GridPane.rowIndex="2" />
                  <TextField fx:id="txtRepartidor" editable="false" layoutX="310.0" layoutY="43.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" />
                  <Label layoutX="10.0" layoutY="77.0" text="Origen:" GridPane.rowIndex="3" />
                  <TextField fx:id="txtOrigen" editable="false" layoutX="310.0" layoutY="73.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" />
                  <Label layoutX="10.0" layoutY="107.0" text="Destino:" GridPane.rowIndex="4" />
                  <TextField fx:id="txtDestino" editable="false" layoutX="310.0" layoutY="103.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4" />
                  <Label layoutX="10.0" layoutY="137.0" text="Peso (kg):" GridPane.rowIndex="5" />
                  <TextField fx:id="txtPeso" editable="false" layoutX="310.0" layoutY="133.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="5" />
                  <Label layoutX="10.0" layoutY="167.0" text="Dimensiones (cm³):" GridPane.rowIndex="6" />
