                  <TextField fx:id="txtFechaCreacion" editable="false" layoutX="310.0" layoutY="253.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="9" />
                  <Label layoutX="10.0" layoutY="287.0" text="Fecha Estimada:" GridPane.rowIndex="10" />
                  <TextField fx:id="txtFechaEstimada" editable="false" layoutX="310.0" layoutY="283.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="10" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </padding>
            </GridPane>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Button fx:id="btnCerrar" mnemonicParsing="false" onAction="#onCerrar" text="Cerrar" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
